<h1>{{ title }}</h1>
<table>
  <tr>
    <td>Name</td>
    <td>Number</td>
  </tr>
  {{#each student}}
    <form method="post">
      <tr>
        <td>{{Name}}</td>
        <td>{{Number}}</td>
        <td style="border: none; margin-left: 10px;">
          <button 
            style="margin-left: 10px;"
            type="submit" 
            name="delBtn"
            value={{Number}} 
            formaction="/delete">
          삭제
          </button>
        </td>
      </tr>    
    </form>
  {{/each}}
</table>