<h1>Employee 데이터 삽입</h1>

<form name="employee" method="post" action='/'> <!--form name은 employee고 method는 post, 데이터는 시작페이지에 넘겨줌-->
    <div>
        <label for="fname">Fname</label>
        <input id="fname" name="fname" type="text" required placeholder="Fname"> <!--Fname -->
    </div>
    <div>
        <label for="minit">Minit</label>
        <input id="minit" name="minit" type="text" required placeholder="Minit"> <!--Middle name -->
    </div>
    <div>
        <label for="lname">Lname</label>
        <input id="lname" name="lname" type="text" required placeholder="Lname"> <!--Lname -->
    </div>
        <div>
        <label for="ssn">Ssn</label>
        <input id="ssn" name="ssn" type="text" required placeholder="Ssn"> <!--Ssn -->
    </div>
        <div>
        <label for="bdate">Bdate</label>
        <input id="bdate" name="bdate" type="text" required placeholder="Bdate"> <!--Bdate -->
    </div>
        <div>
        <label for="address">Address</label>
        <input id="address" name="address" type="text" required placeholder="Address"> <!--Adress -->
    </div>
        <div>
        <label for="sex">Sex</label>
        <input id="sex" name="sex" type="text" required placeholder="Sex"> <!--Sex -->
    </div>
        <div>
        <label for="salary">Salary</label>
        <input id="salary" name="salary" type="text" required placeholder="Salary"> <!--Salary -->
    </div>
        <div>
        <label for="super_ssn">Super_ssn</label>
        <input id="super_ssn" name="super_ssn" type="text" required placeholder="Super_ssn"> <!--Super_ssn -->
    </div>
        <div>
        <label for="dno">Dno</label>
        <input id="dno" name="dno" type="text" required placeholder="Dno"> <!--Dno -->
    </div>
    <div>
        <input type="submit" value="삽입">
    </div>
</form>

